  "scripts": {
    "build": "yarn build:src && yarn build:declarations",
    "build:clean": "yarn clean && yarn build",
    "build:src": "babel -d lib src --extensions .js,.ts,.jsx,.tsx",
    "build:declarations": "tsc --project tsconfig.types.json",
    "clean": "rm -rf lib",
    "lint": "eslint --ext js,jsx,ts,tsx src test storybook",
    "test": "mocha \"test/**/*.unit.jsx\" \"test/**/*.unit.js\" \"test/**/*.unit.tsx\" \"test/**/*.unit.ts\"",
    "storybook": "start-storybook -c ./storybook/config -p 8085",
    "build-storybook": "build-storybook -c ./storybook/config",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
