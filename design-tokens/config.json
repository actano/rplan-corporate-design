{
  "source": ["properties/**/*.json"],
  "platforms": {
    "js": {
      "buildPath": "build/js/",
      "files": [{
        "destination": "color.ts",
        "format": "javascript/es6"
      }],
      "transforms": ["attribute/cti", "name/cti/camel", "color/hex"],
      "filter": {
        "attributes": {
          "category": "color"
        }
      }
    }
  }
}
